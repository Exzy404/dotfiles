{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#ffefba",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "template": "<#ffefba>\ueaf7 </><b>{{ .Path }}</b>",
          "type": "path"
        },
        {
          "foreground": "#43CCEA",
          "properties": {
            "branch_icon": ""
          },
          "style": "diamond",
          "template": "<#ffffff> \u26A1 </><b>{{ .HEAD }}</b>",
          "type": "git"
        },
        {
          "foreground": "#fff",
          "style": "diamond",
          "template": "<#fff> \uf303 </>",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#FFFFFF",
          "style": "plain",
          "template": ">",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
